@model FrontEnd.Models.UsuarioViewModel

@{
    ViewData["Title"] = "EliminarUsuario";
    Layout = "~/Views/_ViewImports.cshtml";
}

<h1>EliminarUsuario</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>UsuarioViewModel</h4>
    <hr />
    <dl class="row">
     
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UsuarioId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nombre)
     
       
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Apellido)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Correo)
        </dt>
        
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Contraseña)
       
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.RolId)
        </dt>
       
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FechaRegistro)
        </dt>

        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Activo)
        </dt>
        
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Direccion)
        </dt>
        
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Telefono)
        </dt>
        
    </dl>
    
    <form asp-action="EliminarUsuario">
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
</div>
